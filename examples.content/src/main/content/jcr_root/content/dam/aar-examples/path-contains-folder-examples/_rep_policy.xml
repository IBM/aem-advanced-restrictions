<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <deny1
        jcr:primaryType="rep:DenyACE"
        rep:principalName="contributor"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            aarPathContainsFolder="private-nt"/>
    </deny1>
    <deny2
            jcr:primaryType="rep:DenyACE"
            rep:principalName="contributor"
            rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
                jcr:primaryType="rep:Restrictions"
                aarPathContainsFolder="private-sling"/>
    </deny2>
    <deny3
            jcr:primaryType="rep:DenyACE"
            rep:principalName="contributor"
            rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
                jcr:primaryType="rep:Restrictions"
                aarPathContainsFolder="private-slingOrdered"/>
    </deny3>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="aarPrivateAccess"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            aarPathContainsFolder="private-nt"/>
    </allow1>
    <allow2
            jcr:primaryType="rep:GrantACE"
            rep:principalName="aarPrivateAccess"
            rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
                jcr:primaryType="rep:Restrictions"
                aarPathContainsFolder="private-sling"/>
    </allow2>
    <allow3
            jcr:primaryType="rep:GrantACE"
            rep:principalName="aarPrivateAccess"
            rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
                jcr:primaryType="rep:Restrictions"
                aarPathContainsFolder="private-slingOrdered"/>
    </allow3>
</jcr:root>
